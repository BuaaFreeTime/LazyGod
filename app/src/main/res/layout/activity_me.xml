<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/home">


    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_below="@id/button34"
        android:background="@drawable/addaphotobutton"
        android:layout_marginTop="9dp"
        android:layout_marginLeft="60dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="130dp"
        android:layout_height="33dp"
        android:layout_marginTop="9dp"
        android:text="Student name"
        android:textSize="24sp"
        android:layout_toRightOf="@id/imageButton2"
        android:layout_below="@id/button34"/>

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="9dp"
        android:text="The University of Sydney"
        android:textColor="@android:color/black"
        android:layout_below="@id/textView5"
        android:layout_toRightOf="@id/imageButton2"/>

    <Button
        android:id="@+id/button34"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/backbutton"
        android:onClick="onAddItemClick"
        android:horizontalSpacing="8dp"
        android:verticalSpacing="8dp"/>


    <Button
        android:id="@+id/button36"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:background="@drawable/walletbutton"
        android:onClick="onAddItemClick"
        android:layout_below="@id/imageButton2" />

    <Button
        android:id="@+id/button35"
        android:layout_width="200dp"
        android:layout_height="200dp"
        android:background="@drawable/draftsbutton"
        android:onClick="onAddItemClick"
        android:layout_below="@id/imageButton2"
        android:layout_toRightOf="@id/button36"/>


    <Button
        android:id="@+id/button37"
        android:layout_width="403dp"
        android:layout_height="40dp"
        android:onClick="onAddItemClick"
        android:background="@drawable/logoutbutton"
        android:layout_below="@id/button39"/>

    <Button
        android:id="@+id/button39"
        android:layout_width="403dp"
        android:layout_height="40dp"
        android:onClick="onAddItemClick"
        android:background="@drawable/aboutbutton"
        android:layout_below="@id/button38"/>

    <Button
        android:id="@+id/button38"
        android:layout_width="403dp"
        android:layout_height="40dp"
        android:onClick="onAddItemClick"
        android:background="@drawable/feedbackbutton"
        android:layout_below="@id/button36"/>

    <Button
        android:id="@+id/button31"
        android:layout_width="125dp"
        android:layout_height="100dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:background="@drawable/runnerbutton2"
        android:onClick="onAddItemClick"/>

    <Button
        android:id="@+id/button32"
        android:layout_width="125dp"
        android:layout_height="100dp"
        android:onClick="onAddItemClick"
        android:background="@drawable/taskbutton"
        android:layout_toRightOf="@id/button31"
        android:layout_alignParentBottom="true" />

    <Button
        android:id="@+id/button33"
        android:layout_width="125dp"
        android:layout_height="100dp"
        android:background="@drawable/mebutton2"
        android:layout_toRightOf="@id/button32"
        android:onClick="onAddItemClick"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"/>




</RelativeLayout>